{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky install",
    "commit": "git-cz",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "format:lint-staged": "prettier --write",
    "lint:check": "eslint --ext .ts,.tsx,.js,.jsx,.vue .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx,.vue --fix .",
    "lint:lint-staged": "eslint --fix",
    "stylelint:check": "stylelint **/*.{scss,css}",
    "stylelint:fix": "stylelint --fix **/*.{scss,css}",
    "stylelint:lint-staged": "stylelint --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@mdi/js": "^7.2.96",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "commitizen": "^4.3.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nuxt": "^3.5.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-standard-scss": "^9.0.0",
    "typescript": "^4.9.5",
    "vite-plugin-vuetify": "^1.0.2",
    "vuetify": "^3.3.1"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
